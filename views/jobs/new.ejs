<% include ../partials/header %>

    <div class="row">
        <h1 class="center">Create a New Jobs</h1>
        <div class="form">
            <form action="/jobs" method="POST">
                <div class="form-group">
                    <label class="label label-info" for="title">Title</label>
                    <input class="form-control" type="text" name="title" placeholder="title">
                </div>
                <div class="form-group">
                    <label class="label label-info" for="description">Description</label>
                    <textarea class="form-control" name="description" placeholder="description" rows="5"></textarea>
                </div>
                <div class="form-group">
                    <button class="btn btn-lg btn-primary btn-block">Submit</button>
                </div>
            </form>
            <a href="/jobs">Go back</a>
        </div>
    </div>

<% include ../partials/footer %>